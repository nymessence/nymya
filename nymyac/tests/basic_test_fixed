// Tests import behavior, basic functionality, and usage of standard libraries

// Test Crystal library
import crystal

func test_crystal() -> Void {
    crystal.manifest("Testing Crystal library...")
}

// Test Math library
import math

func test_math() -> Void {
    crystal.manifest("Testing Math library...")
    
    // Test BigInt
    var big_num = math.BigInt(12345)
    crystal.manifest("Big number: " + big_num.to_string())
    
    // Test Complex numbers
    var c1 = math.Complex(3.0, 4.0)
    var c2 = math.Complex(1.0, 2.0)
    var c3 = c1.add(c2)
    crystal.manifest("Complex addition: " + c3.real + ", " + c3.imag)
    
    // Test vectors
    var v1 = math.vector.Vec3(1.0, 0.0, 0.0)
    var v2 = math.vector.Vec3(0.0, 1.0, 0.0)
    var cross_prod = v1.cross(v2)
    crystal.manifest("Cross product: " + cross_prod.x + ", " + cross_prod.y + ", " + cross_prod.z)
    
    // Test constants
    crystal.manifest("PI value: " + math.PI)
}

// Test Quantum library
import quantum

func test_quantum() -> Void {
    crystal.manifest("Testing Quantum library...")
    
    // Test qubit creation
    var q0 = quantum.Qubit.zero()
    var q1 = quantum.Qubit.one()
    var qplus = quantum.Qubit.plus()
    
    crystal.manifest("Created |0⟩, |1⟩, and |+⟩ qubits")
    
    // Test quantum gates - using direct qubit operation API
    var result = quantum.gate.hadamard(q0)
    crystal.manifest("Applied H gate to |0⟩")

    // Note: shira gate doesn't appear to exist in the current gate library
    crystal.manifest("Note: shira gate is not currently implemented")
}

func main() -> Void {
    test_crystal()
    test_math()
    test_quantum()
    
    crystal.manifest("All tests completed successfully!")
}