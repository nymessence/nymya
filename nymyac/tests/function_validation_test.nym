// Quick test for the functions I implemented to ensure they work properly

import math
import math.hypercalc
import crystal

func main() -> Void {
    crystal.manifest("Testing implemented functions...")
    
    // Test some basic math.hypercalc functions
    var result = math.hypercalc.add(5.0, 3.0)
    crystal.manifest("5 + 3 = " + result)
    
    var mult_result = math.hypercalc.multiply(4.0, 6.0)
    crystal.manifest("4 * 6 = " + mult_result)
    
    var exp_result = math.hypercalc.exp(1.0)
    crystal.manifest("e^1 ≈ " + exp_result)
    
    var ln_result = math.hypercalc.ln(math.E)
    crystal.manifest("ln(e) ≈ " + ln_result)
    
    var pow_result = math.hypercalc.pow(2.0, 8.0)
    crystal.manifest("2^8 = " + pow_result)
    
    var sqrt_result = math.hypercalc.sqrt(16.0)
    crystal.manifest("sqrt(16) = " + sqrt_result)
    
    var sin_result = math.hypercalc.sin(math.PI / 2.0)
    crystal.manifest("sin(π/2) ≈ " + sin_result)
    
    crystal.manifest("All basic tests passed!")
}