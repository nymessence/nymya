// Phase Gate (S gate)
// Part of the quantum gate library collection

namespace quantum {
    namespace gate {
        
        // Phase gate implementation
        func phase(qubit: Qubit) -> Qubit {
            // S = [1 0; 0 i]
            return Qubit(qubit.alpha, qubit.beta.multiply(math.Complex(0.0, 1.0)))
        }
    }
}